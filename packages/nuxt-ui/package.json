{
  "name": "nuxt-ui",
  "type": "module",
  "version": "1.0.0",
  "description": "nuxt-ui for web components with unocss & vueuse",
  "keywords": [
    "nuxt-ui",
    "unocss",
    "vueuse"
  ],
  "license": "MIT",
  "author": "chris-zhu <https://github.com/chris-zhu>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-ui/nuxt-ui.git"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "stub": "tsup --watch"
  },
  "bugs": {
    "url": "https://github.com/nuxt-ui/nuxt-ui/issues"
  },
  "homepage": "https://github.com/nuxt-ui/nuxt-ui#readme",
  "dependencies": {
    "@nuxt-ui/components": "workspace:*"
  }
}
